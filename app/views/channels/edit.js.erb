$('#channel_edit').html('<%= escape_javascript(render :partial =>'channels/edit')%>');
$('#channel_edit').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Editar",
  modal: true,
  buttons: {
                "Guardar": function()
                {
                    $("#form_edit_channel form").submit();
                },
                "Cancelar" : function()
                {
                    $("#channel_edit").dialog("close");
                }
            }
});