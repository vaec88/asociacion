$('#channel_delete').html('<%= escape_javascript(render :partial =>'channels/eliminar')%>');
$('#channel_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_channel form").submit();
                },
                "Cancelar" : function()
                {
                    $("#channel_delete").dialog("close");
                }
            }
});