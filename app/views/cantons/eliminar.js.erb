$('#canton_delete').html('<%= escape_javascript(render :partial =>'cantons/eliminar')%>');
$('#canton_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_canton form").submit();
                },
                "Cancelar" : function()
                {
                    $("#canton_delete").dialog("close");
                }
            }
});