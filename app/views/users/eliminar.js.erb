$('#usuario_delete').html('<%= escape_javascript(render :partial =>'users/eliminar')%>');
$('#usuario_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_usuario form").submit();
                },
                "Cancelar" : function()
                {
                    $("#usuario_delete").dialog("close");
                }
            }
});