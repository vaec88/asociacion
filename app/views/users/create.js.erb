document.getElementById("check_usu_username").focus();

function trim(matricula, pagina_web, apellido, nombre, direccion)
{
var matr = matricula.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
var pag_web = pagina_web.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
var apell = apellido.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
var nomb = nombre.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
var direc = direccion.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
$("#check_usu_matr_prof").val(matr);
$("#check_usu_pag_web").val(pag_web);
$("#check_pers_apellido").val(apell);
$("#check_pers_nombre").val(nomb);
$("#check_pers_direccion").val(direc);
//alert(retorno);
}

var matricula = document.getElementById("check_usu_matr_prof").value
var pagina_web = document.getElementById("check_usu_pag_web").value
var apellido = document.getElementById("check_pers_apellido").value
var nombre = document.getElementById("check_pers_nombre").value
var direccion = document.getElementById("check_pers_direccion").value
trim(matricula, pagina_web, apellido, nombre, direccion);

