$('#image_delete').html('<%= escape_javascript(render :partial =>'images/eliminar')%>');
$('#image_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_image form").submit();
                },
                "Cancelar" : function()
                {
                    $("#image_delete").dialog("close");
                }
            }
});