$('#solicitude_delete').html('<%= escape_javascript(render :partial =>'solicitudes/eliminar')%>');
$('#solicitude_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_solicitude form").submit();
                },
                "Cancelar" : function()
                {
                    $("#solicitude_delete").dialog("close");
                }
            }
});