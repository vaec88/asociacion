$('#state_delete').html('<%= escape_javascript(render :partial =>'states/eliminar')%>');
$('#state_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_state form").submit();
                },
                "Cancelar" : function()
                {
                    $("#state_delete").dialog("close");
                }
            }
});