$('#piece_delete').html('<%= escape_javascript(render :partial =>'pieces/eliminar')%>');
$('#piece_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_piece form").submit();
                },
                "Cancelar" : function()
                {
                    $("#piece_delete").dialog("close");
                }
            }
});