$('#parameter_edit').html('<%= escape_javascript(render :partial =>'parameters/edit')%>');
$('#parameter_edit').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Editar",
  modal: true,
  buttons: {
                "Guardar": function()
                {
                    $("#form_edit_parameter form").submit();
                },
                "Cancelar" : function()
                {
                    $("#parameter_edit").dialog("close");
                }
            }
});