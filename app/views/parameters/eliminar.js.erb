$('#parameter_delete').html('<%= escape_javascript(render :partial =>'parameters/eliminar')%>');
$('#parameter_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_parameter form").submit();
                },
                "Cancelar" : function()
                {
                    $("#parameter_delete").dialog("close");
                }
            }
});