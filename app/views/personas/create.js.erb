function trim(apellido, nombre, direccion)
{
var apell = apellido.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
var nomb = nombre.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
var direc = direccion.replace(/^(\s)*/,"").replace(/(\s)*$/,"").replace(/(\s+)/g," ");
$("#pers_demand_apellidos").val(apell);
$("#pers_demand_nombres").val(nomb);
$("#pers_demand_direccion").val(direc);
//alert(retorno);
}

var apellido = document.getElementById("pers_demand_apellidos").value
var nombre = document.getElementById("pers_demand_nombres").value
var direccion = document.getElementById("pers_demand_direccion").value
trim(apellido, nombre, direccion);