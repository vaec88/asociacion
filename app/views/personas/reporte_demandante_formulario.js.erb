//alert("Aqui va el formulario");
$('#usuario_reporte_demandante').html('<%= escape_javascript(render :partial =>'personas/reporte_demandante_formulario')%>');
$('#usuario_reporte_demandante').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Reporte de Demandantes",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_reporte_demandante form").submit();
                    $("#usuario_reporte_demandante").dialog("close");
                },
                "Cancelar" : function()
                {
                    $("#usuario_reporte_demandante").dialog("close");
                }
            }
});