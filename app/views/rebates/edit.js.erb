$('#rebate_edit').html('<%= escape_javascript(render :partial =>'rebates/edit')%>');
$('#rebate_edit').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Editar",
  modal: true,
  buttons: {
                "Guardar": function()
                {
                    $("#form_edit_rebate form").submit();
                },
                "Cancelar" : function()
                {
                    $("#rebate_edit").dialog("close");
                }
            }
});