$('#rebate_delete').html('<%= escape_javascript(render :partial =>'rebates/eliminar')%>');
$('#rebate_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_rebate form").submit();
                },
                "Cancelar" : function()
                {
                    $("#rebate_delete").dialog("close");
                }
            }
});