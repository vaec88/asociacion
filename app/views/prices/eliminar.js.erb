$('#price_delete').html('<%= escape_javascript(render :partial =>'prices/eliminar')%>');
$('#price_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_price form").submit();
                },
                "Cancelar" : function()
                {
                    $("#price_delete").dialog("close");
                }
            }
});