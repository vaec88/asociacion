$('#role_delete').html('<%= escape_javascript(render :partial =>'roles/eliminar')%>');
$('#role_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_role form").submit();
                },
                "Cancelar" : function()
                {
                    $("#role_delete").dialog("close");
                }
            }
});