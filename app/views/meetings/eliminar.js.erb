$('#meeting_delete').html('<%= escape_javascript(render :partial =>'meetings/eliminar')%>');
$('#meeting_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_meeting form").submit();
                },
                "Cancelar" : function()
                {
                    $("#meeting_delete").dialog("close");
                }
            }
});