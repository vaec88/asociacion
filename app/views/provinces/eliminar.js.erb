$('#province_delete').html('<%= escape_javascript(render :partial =>'provinces/eliminar')%>');
$('#province_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_province form").submit();
                },
                "Cancelar" : function()
                {
                    $("#province_delete").dialog("close");
                }
            }
});