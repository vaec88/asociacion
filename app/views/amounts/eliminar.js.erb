$('#amount_delete').html('<%= escape_javascript(render :partial =>'amounts/eliminar')%>');
$('#amount_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_amount form").submit();
                },
                "Cancelar" : function()
                {
                    $("#amount_delete").dialog("close");
                }
            }
});