$('#parish_delete').html('<%= escape_javascript(render :partial =>'parishes/eliminar')%>');
$('#parish_delete').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Eliminar",
  modal: true,
  buttons: {
                "Aceptar": function()
                {
                    $("#form_eliminar_parish form").submit();
                },
                "Cancelar" : function()
                {
                    $("#parish_delete").dialog("close");
                }
            }
});