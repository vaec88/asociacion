$('#parish_edit').html('<%= escape_javascript(render :partial =>'parishes/edit')%>');
$('#parish_edit').dialog({
  resizable: false,
  height: 'auto',
  width: 'auto',
  title: "Editar",
  modal: true,
  buttons: {
                "Guardar": function()
                {
                    $("#form_edit_parish form").submit();
                },
                "Cancelar" : function()
                {
                    $("#parish_edit").dialog("close");
                }
            }
});